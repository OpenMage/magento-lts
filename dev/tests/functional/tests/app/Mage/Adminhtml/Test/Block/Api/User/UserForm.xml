<tabs>
    <user-info>
        <class>\Mage\Adminhtml\Test\Block\Widget\Tab</class>
        <selector>#page_tabs_main_section</selector>
        <strategy>css selector</strategy>
        <fields>
            <username />
            <firstname />
            <lastname />
            <email />
            <password>
                <selector>#user_password</selector>
            </password>
            <password_confirmation>
                <selector>#user_confirmation</selector>
            </password_confirmation>
            <current_password />
            <is_active>
                <input>select</input>
            </is_active>
        </fields>
    </user-info>
    <user-role>
        <class>\Mage\Adminhtml\Test\Block\Api\User\Tab\Role</class>
        <selector>#page_tabs_roles_section</selector>
        <strategy>css selector</strategy>
    </user-role>
</tabs>
