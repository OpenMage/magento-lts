<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../../../vendor/magento/mtf/etc/variations.xsd">
    <testCase name="Mage\Catalog\Test\TestCase\Product\AddUpSellProductsEntityTest" summary="AddUpSellProductsEntityTest">
        <variation name="AddUpSellProductsEntityTestVariation1" firstConstraint="Mage\Catalog\Test\Constraint\AssertProductSaveMessage" method="test">
            <data name="productData" xsi:type="string">catalogProductSimple::default</data>
            <data name="upSellProductsData" xsi:type="string">catalogProductSimple::default, catalogProductSimple::default</data>
            <constraint name="Mage\Catalog\Test\Constraint\AssertProductSaveMessage" next="Mage\Catalog\Test\Constraint\AssertProductInGrid"/>
            <constraint name="Mage\Catalog\Test\Constraint\AssertProductInGrid" next="Mage\Catalog\Test\Constraint\AssertProductForm" prev="Mage\Catalog\Test\Constraint\AssertProductSaveMessage"/>
            <constraint name="Mage\Catalog\Test\Constraint\AssertProductForm" next="Mage\Catalog\Test\Constraint\AssertUpSellProductsSection" prev="Mage\Catalog\Test\Constraint\AssertProductInGrid"/>
            <constraint name="Mage\Catalog\Test\Constraint\AssertUpSellProductsSection" prev="Mage\Catalog\Test\Constraint\AssertProductForm"/>
        </variation>
        <variation name="AddUpSellProductsEntityTestVariation2" firstConstraint="Mage\Catalog\Test\Constraint\AssertProductSaveMessage" method="test">
            <data name="productData" xsi:type="string">catalogProductSimple::default</data>
            <data name="upSellProductsData" xsi:type="string">catalogProductSimple::default, configurableProduct::default</data>
            <constraint name="Mage\Catalog\Test\Constraint\AssertProductSaveMessage" next="Mage\Catalog\Test\Constraint\AssertProductInGrid"/>
            <constraint name="Mage\Catalog\Test\Constraint\AssertProductInGrid" next="Mage\Catalog\Test\Constraint\AssertProductForm" prev="Mage\Catalog\Test\Constraint\AssertProductSaveMessage"/>
            <constraint name="Mage\Catalog\Test\Constraint\AssertProductForm" next="Mage\Catalog\Test\Constraint\AssertUpSellProductsSection" prev="Mage\Catalog\Test\Constraint\AssertProductInGrid"/>
            <constraint name="Mage\Catalog\Test\Constraint\AssertUpSellProductsSection" prev="Mage\Catalog\Test\Constraint\AssertProductForm"/>
        </variation>
        <variation name="AddUpSellProductsEntityTestVariation3" firstConstraint="Mage\Catalog\Test\Constraint\AssertProductSaveMessage" method="test">
            <data name="productData" xsi:type="string">configurableProduct::default</data>
            <data name="upSellProductsData" xsi:type="string">catalogProductSimple::default, configurableProduct::default</data>
            <constraint name="Mage\Catalog\Test\Constraint\AssertProductSaveMessage" next="Mage\Catalog\Test\Constraint\AssertProductInGrid"/>
            <constraint name="Mage\Catalog\Test\Constraint\AssertProductInGrid" next="Mage\Catalog\Test\Constraint\AssertConfigurableProductForm" prev="Mage\Catalog\Test\Constraint\AssertProductSaveMessage"/>
            <constraint name="Mage\Catalog\Test\Constraint\AssertConfigurableProductForm" next="Mage\Catalog\Test\Constraint\AssertUpSellProductsSection" prev="Mage\Catalog\Test\Constraint\AssertProductInGrid"/>
            <constraint name="Mage\Catalog\Test\Constraint\AssertUpSellProductsSection" prev="Mage\Catalog\Test\Constraint\AssertConfigurableProductForm"/>
        </variation>
    </testCase>
</config>
