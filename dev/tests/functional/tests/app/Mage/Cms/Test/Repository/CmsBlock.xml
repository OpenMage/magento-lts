        xsi:noNamespaceSchemaLocation="../../../../../../vendor/magento/mtf/Magento/Mtf/Repository/etc/repository.xsd">
    <repository class="Mage\Cms\Test\Repository\CmsBlock">
        <dataset name="default">
            <field name="title" xsi:type="string">cms-block-title-%isolation%</field>
            <field name="identifier" xsi:type="string">cms-block-identifier-%isolation%</field>
            <field name="content" xsi:type="array">
                <item name="content" xsi:type="string">cms block text content</item>
            </field>
            <field name="is_active" xsi:type="string">Enabled</field>
        </dataset>
    </repository>
</config>
