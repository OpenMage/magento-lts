<mapping strict="0">
    <fields>
        <order_id>
            <selector>[name='oar_order_id']</selector>
        </order_id>
        <billing_last_name>
            <selector>[name='oar_billing_lastname']</selector>
        </billing_last_name>
        <find_order_by>
            <selector>[name='oar_type']</selector>
            <input>select</input>
        </find_order_by>
        <email_address>
            <selector>[name='oar_email']</selector>
        </email_address>
        <billing_zip_code>
            <selector>[name='oar_zip']</selector>
        </billing_zip_code>
    </fields>
</mapping>
