<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../../vendor/magento/mtf/Magento/Mtf/Repository/etc/repository.xsd">
    <repository class="Mage\Core\Test\Repository\ConfigData">
        <dataset name="config_currency_symbols_usd_and_uah">
            <field name="currency/options/allow" xsi:type="array">
                <item name="scope" xsi:type="string">currency</item>
                <item name="scope_id" xsi:type="number">1</item>
                <item name="label" xsi:type="string" />
                <item name="value" xsi:type="array">
                    <item name="US Dollar" xsi:type="string">USD</item>
                    <item name="Ukrainian Hryvnia" xsi:type="string">UAH</item>
                </item>
            </field>
        </dataset>
        <dataset name="config_currency_symbols_usd">
            <field name="currency/options/allow" xsi:type="array">
                <item name="scope" xsi:type="string">currency</item>
                <item name="scope_id" xsi:type="number">1</item>
                <item name="label" xsi:type="string" />
                <item name="value" xsi:type="array">
                    <item name="US Dollar" xsi:type="string">USD</item>
                </item>
            </field>
        </dataset>
        <dataset name="config_currency_converters">
            <field name="currency/fixerio/api_key" xsi:type="array">
                <item name="scope" xsi:type="string">currency</item>
                <item name="scope_id" xsi:type="number">1</item>
                <item name="label" xsi:type="string" />
                <item name="value" xsi:type="string" />
            </field>
            <field name="currency/currencyconverterapi/api_key" xsi:type="array">
                <item name="scope" xsi:type="string">currency</item>
                <item name="scope_id" xsi:type="number">1</item>
                <item name="label" xsi:type="string" />
                <item name="value" xsi:type="string" />
            </field>
        </dataset>
    </repository>
</config>
