            <constraint name="Mage\Admin\Test\Constraint\AssertUserSuccessLogOut" next="Mage\Admin\Test\Constraint\AssertUserAccessDeniedMessage" prev="Mage\Admin\Test\Constraint\AssertUserInGrid" />
