<config>
    <modules>
        <Mage_Connect>
            <version>1.6.0.0</version>
        </Mage_Connect>
    </modules>
    <global>
        <models>
            <connect>
                <class>Mage_Connect_Model</class>
            </connect>
        </models>
        <blocks>
            <connect>
                <class>Mage_Connect_Block</class>
            </connect>
        </blocks>
    </global>
    <admin>
        <routers>
            <adminhtml>
                <args>
                    <modules>
                        <connect before="Mage_Adminhtml">Mage_Connect_Adminhtml</connect>
                    </modules>
                </args>
            </adminhtml>
        </routers>
    </admin>
    <adminhtml>
        <layout>
            <updates>
                <connect>
                    <file>connect.xml</file>
                </connect>
            </updates>
        </layout>
    </adminhtml>
</config>
