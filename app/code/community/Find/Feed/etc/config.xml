<config>
    <modules>
        <Find_Feed>
            <version>0.0.2</version>
        </Find_Feed>
    </modules>
    <global>
        <models>
            <find_feed>
                <class>Find_Feed_Model</class>
                <resourceModel>find_feed_mysql4</resourceModel>
            </find_feed>
            <find_feed_mysql4>
                <class>Find_Feed_Model_Mysql4</class>
                <entities>
                    <feed_import_codes>
                        <table>find_feed_import_codes</table>
                    </feed_import_codes>
                </entities>
            </find_feed_mysql4>
        </models>
        <resources>
            <find_feed_setup>
                <setup>
                    <module>Find_Feed</module>
                    <class>Find_Feed_Model_Mysql4_Setup</class>
                </setup>
            </find_feed_setup>
        </resources>
        <blocks>
            <find_feed><class>Find_Feed_Block</class></find_feed>
        </blocks>
        <helpers>
            <find_feed>
                <class>Find_Feed_Helper</class>
            </find_feed>
        </helpers>
    </global>
    <adminhtml>
        <events>
            <admin_system_config_changed_section_feed>
                <observers>
                    <find_feed>
                        <class>find_feed/observer</class>
                        <method>saveSystemConfig</method>
                    </find_feed>
                </observers>
            </admin_system_config_changed_section_feed>
        </events>
        <acl>
            <resources>
                <admin>
                    <children>
                        <system>
                            <children>
                                <config>
                                    <children>
                                        <feed>
                                            <title>Feed</title>
                                        </feed>
                                    </children>
                                </config>
                            </children>
                        </system>
                    </children>
                </admin>
            </resources>
        </acl>
    </adminhtml>
    <crontab>
        <jobs>
            <find_feed_file_send>
                <schedule>
                    <config_path>feed/settings/cron_schedule</config_path>
                </schedule>
                <run>
                    <model>find_feed/import::dispatch</model>
                </run>
            </find_feed_file_send>
        </jobs>
    </crontab>
    <admin>
        <routers>
             <adminhtml>
                 <args>
                     <modules>
                         <find_feed before="Mage_Adminhtml">Find_Feed_Adminhtml</find_feed>
                     </modules>
                 </args>
             </adminhtml>
        </routers>
    </admin>
    <stores>
        <admin>
            <design>
                <theme>
                    <default>find</default>
                </theme>
            </design>
        </admin>
    </stores>
    <adminhtml>
        <layout>
            <updates>
                <find_feed>
                    <file>feed.xml</file>
                </find_feed>
            </updates>
        </layout>
    </adminhtml>
